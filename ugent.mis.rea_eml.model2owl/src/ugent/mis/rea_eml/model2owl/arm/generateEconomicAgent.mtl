[comment encoding = UTF-8 /]
[module generateEconomicAgent('REA_EML_AR')]
[template public generateElement(anEconomicAgent : EconomicAgent, aREA_EML_ARM : REA_EML_ARM)]
 <owl:NamedIndividual rdf:about="&[aREA_EML_ARM.ontologyNS/];[anEconomicAgent.name/]">
        <rdf:type rdf:resource="&REA-EMO;EconomicAgent"/>
[for (aParticipation : participation | aREA_EML_ARM.participations)]
	[if (aParticipation.economicAgent = anEconomicAgent)]
		<REA-EMO:participationEconomicEvent_EconomicEvent rdf:resource="&[aREA_EML_ARM.ontologyNS/];[aParticipation.economicEvent.name/]"/>
	[/if]
[/for]
 </owl:NamedIndividual>
[/template]
